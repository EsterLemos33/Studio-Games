primeiranota = float(input("Digite sua primeira nota:"))
if 0 <= primeiranota <= 10:
    segundanota = float(input("Digite sua segunda nota:"))
    if 0 <= segundanota <= 10:
        terceiranota = float(input("Digite sua terceira nota:"))
        if 0 <= terceiranota <= 10:
            media = (primeiranota + segundanota + terceiranota) / 3
            if media < 5:
                print(f"Voce tirou {media}, e foi reprovado.")
            elif media < 10:
                print(f"Voce tirou {media}, e foi aprovado.")
            else:
                print(f"Voce tirou {media}, uma nota perfeita, e foi aprovado com excelencia.")
        else:
                print("Digite uma nota n, tal que 0 <= n <= 10")
    else:
        print("Digite uma nota n, tal que 0 <= n <= 10")
else:
    print("Digite uma nota n, tal que 0 <= n <= 10")
